{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "OrderLine",
  "type": "object",
  "properties": {
    "IDP": { "type": "number" },
    "IDC": { "type": "number" },
    "date": { "type": "string", "format": "date-time" },
    "quantity": { "type": "number" },
    "deliveryDate": { "type": "string", "format": "date-time" },
    "comment": { "type": "string" },
    "grade": { "type": "number" }
  },
  "required": [
    "IDP",
    "IDC",
    "date",
    "quantity",
    "deliveryDate"
  ]
}
